<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="a1x">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import a1x urdf file -->
    <xacro:include filename="$(find a1x)/urdf/a1x.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="a1x.ros2_control.xacro" />


    <xacro:a1x_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
